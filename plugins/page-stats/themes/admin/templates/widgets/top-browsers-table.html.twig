<table class="table table-hover">
    <tr>
        <th>
            Browser

        </th>
        <th>
            % share
        </th>

    </tr>
    {% for s in data %}
        <tr>
            <td>
                {{ s.browser }}
            </td>
            <td>
                <span title="{{ s.hits }} views">{{ s.share }}
                    %</span>

            </td>

        </tr>
    {% endfor %}
</table>
